{
  "env": {
    "node": true
  },
  "rules": {
    "curly": [2, "multi-line"],
    "consistent-return": 0,
    "eqeqeq": 0,
    "new-cap": 0,
    "no-path-concat": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "never"],
    "strict": [2, "never"]
  }
}
